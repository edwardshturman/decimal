{
  "name": "decimal",
  "author": "Edward Shturman",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "description": "A minimalist, web-based expense tracker",
  "repository": {
    "type": "git",
    "url": "https://github.com/edwardshturman/decimal.git"
  },
  "private": true,
  "scripts": {
    "predev": "bun install",
    "postinstall": "prisma generate",
    "dev": "next dev --turbopack",
    "pretty": "prettier . --check",
    "lint": "eslint .",
    "prebuild": "bun run pretty && bun run lint",
    "prepare": "husky",
    "build": "next build --turbopack",
    "start": "next start",
    "migrate": "dotenv -e .env.local prisma migrate dev",
    "refresh": "git pull && vc env pull"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@prisma/client": "6.14.0",
    "@radix-ui/react-roving-focus": "1.1.11",
    "motion": "12.23.15",
    "next": "15.5.0",
    "next-auth": "5.0.0-beta.25",
    "plaid": "38.0.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-plaid-link": "4.1.1",
    "react-random-reveal": "2.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@types/node": "24.3.0",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "dotenv-cli": "10.0.0",
    "eslint": "9.33.0",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "prettier": "3.6.2",
    "prisma": "6.14.0",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": "24.x"
  }
}
